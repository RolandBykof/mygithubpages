{
  "manifest_version": 3,
  "name": "LinkedIn H4 Navigation",
  "version": "1.0",
  "description": "Lisää h4-otsikot LinkedIn-syötejulkaisuihin ruudunlukijanavigointia varten",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/feed/*", "https://www.linkedin.com/feed"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
